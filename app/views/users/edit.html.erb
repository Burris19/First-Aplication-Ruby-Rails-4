<!DOCTYPE html>
<html>
<head>
	<title>Curso Ruby and Rails</title>
</head>
<body>

	<h1>Editando usario</h1>

	<%= form_for(@usuario) do |f| %>

		<div class="fiel">
			<%= f.label :nombre %> <br>
			<%= f.text_field :nombre %>
		</div>

		<div class="fiel">
			<%= f.label :email %> <br>	
			<%= f.text_field :email%>
		</div>

		<div class="actions">
			<%= f.submit 'Guardar' %>
		</div>

	<% end %>

	<%= link_to 'Atras', users_path %>

</body>
</html>